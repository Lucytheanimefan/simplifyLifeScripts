{
    "extends": "google",
    "env": {
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "object-curly-spacing": 0,
        "arrow-parens": 0,
        "require-jsdoc": 0,
        "max-len": 1,
        "new-cap": 0
    }
}